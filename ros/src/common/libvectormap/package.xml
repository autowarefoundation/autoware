<?xml version="1.0"?>
<package format="2">
    <name>libvectormap</name>
    <version>1.10.0</version>
    <description>Common Library to Read Vector Map</description>
    <maintainer email="sujiwo@ertl.jp">sujiwo</maintainer>
    <license>Apache 2</license>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>autoware_build_flags</buildtool_depend>

    <build_depend>vector_map</build_depend>
    <build_depend>vector_map_msgs</build_depend>
    <build_depend>cmake_modules</build_depend>
    <build_depend>eigen</build_depend>

    <exec_depend>cmake_modules</exec_depend>
    <exec_depend>vector_map</exec_depend>
    <exec_depend>vector_map_msgs</exec_depend>
    <exec_depend>eigen</exec_depend>

    <export>
        <build_type>ament_cmake</build_type>
    </export>
</package>
