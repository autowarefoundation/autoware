<!-- -->
<launch>
	<arg name="distance_threshold" default="5.0" />
	<arg name="lane_change_interval" default="2" />

	<node pkg="lane_planner" type="lane_select" name="lane_select" output="screen">
	    <param name="distance_threshold" value="$(arg distance_threshold)" />
	    <param name="lane_change_interval" value="$(arg lane_change_interval)" />
	</node>
	
</launch>